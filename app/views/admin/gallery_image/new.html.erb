<html>
<head>
  <title>Department Phonebook</title>
  	<%= stylesheet_link_tag    "admin/admin" %>
	<%= javascript_include_tag  "admin/admin" %>
	<%= yield :scripts %> 
	<%= stylesheet_link_tag 'gmaps4rails' %>
	<%= csrf_meta_tags %>
</head>
<body class="blog">
	<%= render 'admin/header' %>
	<div style="margin-left: 30px; margin-right: 30px;">
	<br>	
		<% unless @page_caching %>	
			<% flash.each do |key, value| %>
				<br>
				<div class="alert alert-<%= key %>" id="alert-flash"><%= value %>
					<a class="close" data-dismiss="alert" href="#">&times;</a>
				</div>
				
			<% end %>
		<% end %>
		<%= yield %>




	</div>

<div class="span12">
			<section id="wizard">
			  <div class="page-header">
	            <h1>Uploade Images to gallery</h1>
	          </div>
 

<%= form_for :images, :html => {:controller => :gallery_image, :multipart => true },:url => {:action => :create} do |c| %>


<!-- 			 		<%= c.label :caption %> 
   -->

				<!-- <input id="member_name" class="height35 padding5 span5" type="text" size="30" name="caption">
				<br><br>
				<%= c.label :image_details %>
				<input id="member_name" class="height35 padding5 span5" type="text" size="30" name=":image_details">
				<br><br> -->
				
					<%= c.label :photo %>
					<%= c.file_field :photo %>
			
				

			<br><br>
			<%= c.submit "Upload Image" %>

<%end%>
</section>
</div>
	
</body>
<style type="text/css">
	header .menu{
	    line-height: 40px; background-color: #EEEEEE; text-align: center; margin: 5px; border-radius: 3px 3px 3px 3px; font-weight: bold;
	}
	header .menu:hover{
	    background-color: #E0E0DF;
	}   
	.padding10{
	    padding: 10px;      
	}
	.padding15{
	    padding: 15px;      
	}
	.padding5{
	    padding: 5px;
	}
	.height35{
	    height: 35px;
	}
	.lineheight25{
	    line-height: 25px;
	}
</style>


</html>






			
