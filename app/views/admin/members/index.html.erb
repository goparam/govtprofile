		<div class="row">
			<div class="span6">
				<h1><%= @title %></h1>
				<p><%= @desc %></p>
			</div>
			<div class="span6">
				<input type="button" value="Add New" class="padding10" onclick="window.location.href='/admin/members/new';">
				<input type="button" value="Add New In English" class="padding10" onclick="window.location.href='/admin/english/new';">
			</div>
		</div>
		<br>



		<style type="text/css" media="screen">
			
			@import "/assets/admin/site_jui.css";
      @import "/assets/admin/demo_table.css";
      @import "/assets/admin/jquery-ui-1.7.2.custom.css";
			
			/*
			 * Override styles needed due to the mix of three different CSS sources! For proper examples
			 * please see the themes example in the 'Examples' section of this site
			 */
			
		</style>
		
		<script src="/assets/admin/jquery.js" type="text/javascript"></script>
		<script src="/assets/admin/jquery.dataTables.nightly.js" type="text/javascript"></script>

		
		<script type="text/javascript">
			function fnFeaturesInit ()
			{
				/* Not particularly modular this - but does nicely :-) */
				$('ul.limit_length>li').each( function(i) {
					if ( i > 10 ) {
						this.style.display = 'none';
					}
				} );
				
				$('ul.limit_length').append( '<li class="css_link">Show more<\/li>' );
				$('ul.limit_length li.css_link').click( function () {
					$('ul.limit_length li').each( function(i) {
						if ( i > 5 ) {
							this.style.display = 'list-item';
						}
					} );
					$('ul.limit_length li.css_link').css( 'display', 'none' );
				} );
			}
			
			$(document).ready( function() {
				fnFeaturesInit();
				$('#example').dataTable( {
					"bJQueryUI": true,
					"sPaginationType": "full_numbers"
				} );
				
				
			} );
		</script>
		

		<div id="fw_container">
			

<table cellpadding="0" cellspacing="0" border="0" class="display" id="example" style="width:1200px">

		<thead>
				<tr>
					
					<th >Name
					<th >अधिकाऱ्यांचे नांव
					<th >Email
					
					<th >phone no
					
					<th >Photo
				</tr>
				</thead>
				<tbody>	
			<% @members.each do |c| %>
				<tr class="gradeA">
					<%
					@profiles=c.profiles
					 @profiles.each do |profiles| 
						if profiles['language']=="M"
							@Profile_M=	profiles
						else 
							@Profile_E=	profiles
						end
					end
					%>
					<td><br><br><a href=/admin/members/<%= c.id%>><%=  @Profile_E.name%></a><br><br>
					<td><%=  @Profile_M.name%>
					<td><%= c.email %>
					
					
					<td>
						<% 
						
						@phones=JSON.parse(c.phones) rescue""
						@phone_arry=[]
						%>

						<% 
						if !@phones.blank?
							@phones.each do |phone| %>
							<%@phone_arry<<phone[1]%>
							<% end 
						end%>
						<%= @phone_arry.join("/<br>").html_safe %>
					<td>
						<% if !c.photo.blank? %>
						<img src=<%=c.photo%> width=60 >
						<% end %>
						
					
				</tr>
			<% end %>
			</tbody>
		</table>